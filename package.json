  "license": "ISC",
  "bugs": {
    "url": "https://denisshamanskiy.github.io/react-demo-weather/issues"
  },
  "homepage": "https://denisshamanskiy.github.io/react-demo-weather/",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.3.0"
  }
}
